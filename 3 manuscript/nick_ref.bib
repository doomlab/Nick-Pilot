issn = {1939-1285},
journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
keywords = {affect decision making is,and,and delayed consequences,decision making,decision making is a,decisions have both immediate,dual task,everyday lives,of,our,recurring task in our,reinforcement learning,understanding the mechanisms that,win-stay lose-shift,working memory load},
number = {6},
pages = {1640--1658},
pmid = {22545616},
title = {{Working-memory load and temporal myopia in dynamic decision making.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028146},
volume = {38},
year = {2012}
}
