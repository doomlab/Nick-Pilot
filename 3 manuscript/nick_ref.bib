pmid = {27643793},
title = {{Measuring adult picky eating: The development of a multidimensional self-report instrument.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27643793 http://doi.apa.org/getdoi.cfm?doi=10.1037/pas0000387},
