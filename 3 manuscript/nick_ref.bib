journal = {European Journal of Personality},
keywords = {Mediation,Physical attractiveness,Relationships,Self-esteem,Sociometer},
month = {jun},
number = {4},
pages = {307--328},
pmid = {23353113},
title = {{Examining dispositional and situational effects on outgroup attitudes}},
url = {http://doi.wiley.com/10.1002/per.710},
volume = {23},
year = {2009}
}
