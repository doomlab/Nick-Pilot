pages = {886--903},
publisher = {Cognitive, Affective, {\&} Behavioral Neuroscience},
title = {{Women in the midluteal phase of the menstrual cycle have difficulty suppressing the processing of negative emotional stimuli: An event-related potential study}},
url = {http://link.springer.com/10.3758/s13415-017-0520-1},
volume = {17},
