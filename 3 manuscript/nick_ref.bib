publisher = {Elsevier},
title = {{Peer and teacher influences on the motivational climate in physical education: A longitudinal perspective on achievement goal adoption}},
url = {http://dx.doi.org/10.1016/j.cedpsych.2017.08.001 http://linkinghub.elsevier.com/retrieve/pii/S0361476X17303843},
volume = {51},
