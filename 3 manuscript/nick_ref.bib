number = {4},
pages = {288--306},
publisher = {Elsevier Inc.},
title = {{Examining transfer effects from dialogic discussions to new tasks and contexts}},
url = {http://dx.doi.org/10.1016/j.cedpsych.2012.02.003},
volume = {37},
year = {2012}
}
