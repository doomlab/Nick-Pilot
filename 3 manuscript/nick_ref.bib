@article{Applebaum2012,
abstract = {OBJECTIVE: A strong therapeutic alliance has been found to predict psychotherapeutic treatment success across a variety of therapeutic modalities and patient populations. However, only a few studies have examined therapeutic alliance as a predictor of psychotherapy outcome among cancer survivors, and none have examined this relation in telephone-administered cognitive-behavioral therapy (T-CBT). This study evaluated the extent to which therapeutic alliance affected psychotherapy outcomes in survivors of hematopoietic stem cell transplantation (HSCT), a treatment for some cancers.$\backslash$n$\backslash$nMETHOD: Forty-six patients enrolled in a randomized clinical trial of T-CBT for posttraumatic stress disorder (PTSD) completed a baseline assessment (including self-report measures of PTSD symptoms, depression, and general distress), 10 individual T-CBT sessions, and follow-up assessments at 6, 9, and 12 months post-baseline. Therapeutic alliance was assessed after the 3rd T-CBT session with the Working Alliance Inventory, which yields overall and subscale (task, bond, and goal) scores.$\backslash$n$\backslash$nRESULTS: Analyses revealed that higher total therapeutic alliance scores prospectively predicted decreased depressive symptomatology; higher task scores predicted decreased overall distress, depressive symptomatology, symptoms of re-experiencing, and avoidance; and higher bond scores predicted decreased depressive symptomatology and symptoms of re-experiencing.$\backslash$n$\backslash$nCONCLUSIONS: These results suggest that assessments of therapeutic alliance should be incorporated into routine clinical care, and therapeutic alliance should be specifically cultivated in interventions to maximize psychotherapeutic benefits involving vulnerable populations such as cancer survivors.},
author = {Applebaum, Allison J. and DuHamel, Katherine N. and Winkel, Gary and Rini, Christine and Greene, Paul B. and Mosher, Catherine E. and Redd, William H.},
doi = {10.1037/a0027956},
isbn = {1939-2117},
issn = {1939-2117},
journal = {Journal of Consulting and Clinical Psychology},
keywords = {1991,cancer,cessful therapy across patient,e,established reliable predictor of,g,horvath,including cognitive and behav-,populations and treatment modalities,ptsd,suc-,symonds,telephone administered cbt,therapeutic alliance,therapeutic alliance is an},
number = {5},
pages = {811--816},
pmid = {22468908},
title = {{Therapeutic alliance in telephone-administered cognitive–behavioral therapy for hematopoietic stem cell transplant survivors.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0027956},
volume = {80},
year = {2012}
}
@article{Cohen2003d,
abstract = {Four studies demonstrated both the power of group influence in persuasion and people's blindness to it. Even under conditions of effortful processing, attitudes toward a social policy depended almost exclusively upon the stated position of one's political party. This effect overwhelmed the impact of both the policy's objective content and participants' ideological beliefs (Studies 1-3), and it was driven by a shift in the assumed factual qualities of the policy and in its perceived moral connotations (Study 4). Nevertheless, participants denied having been influenced by their political group, although they believed that other individuals, especially their ideological adversaries, would be so influenced. The underappreciated role of social identity in persuasion is discussed.},
author = {Cohen, Geoffrey L.},
doi = {10.1037/0022-3514.85.5.808},
isbn = {0022-3514$\backslash$r1939-1315},
