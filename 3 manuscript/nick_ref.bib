isbn = {1076-8971},
issn = {1939-1528},
journal = {Psychology, Public Policy, and Law},
keywords = {admonition,ecological,eyewitness-identification,field,identification purposes,in day-to-day,practice,showup,showups are routinely conducted,showups involve showing a,single individual to a,usually live in the,validity,when police are in,witness for},
number = {1},
pages = {1--22},
title = {{An examination of showups conducted by law enforcement using a field-simulation paradigm.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/law0000115},
volume = {23},
year = {2017}
