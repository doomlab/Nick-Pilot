keywords = {Infant language acquisition,Speech segmentation,Statistical learning,Word learning},
number = {2},
pages = {93--106},
pmid = {21762650},
publisher = {Elsevier Inc.},
title = {{Linking sounds to meanings: Infant statistical learning in a natural language}},
