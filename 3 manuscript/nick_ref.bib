title = {{Moral identity and emotion in athletes}},
url = {http://ezproxy.umsl.edu/login?url=http://search.ebscohost.com/login.aspx?direct=true{\&}db=psyh{\&}AN=2012-32681-001{\&}site=ehost-live{\&}scope=site http://journals.humankinetics.com/doi/10.1123/jsep.34.6.695},
