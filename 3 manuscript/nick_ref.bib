issn = {0162895X},
journal = {Political Psychology},
keywords = {Morality,Political psychology,Right-wing authoritarianism,Social dominance orientation},
month = {aug},
number = {4},
pages = {589--610},
title = {{Mapping the Connections between Politics and Morality: The Multiple Sociopolitical Orientations Involved in Moral Intuition}},
url = {http://doi.wiley.com/10.1111/pops.12006},
volume = {34},
year = {2013}
}
