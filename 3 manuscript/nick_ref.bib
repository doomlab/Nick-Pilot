pages = {1268--1273},
publisher = {Psychonomic Bulletin {\&} Review},
title = {{Divided attention reduces resistance to distraction at encoding but not retrieval}},
url = {http://link.springer.com/10.3758/s13423-016-1210-7},
volume = {24},
year = {2017}
}
