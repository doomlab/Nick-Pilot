volume = {17},
year = {2012}
}
@article{Best2000,
abstract = {Examined the association between perceived warmth of instructor and students' course evaluations on both affective and summative items. Ss were college students taking a cognitive psychology course in 3 consecutive terms. A modified A-B-A design was used to investigate the relation between certain behaviors or statements made in the classroom and the evaluation of the professor's warmth. Consistent with research by H. H. Kelley (1950), student evaluations on affective items were more favorable in those classes in which the professor exhibited the full array of warmth-inducing behaviors. However, increases in perceived warmth were not necessarily related to increases in global effectiveness as measured by a single summative item on the student evaluation instrument.},
