number = {4},
pages = {535--551},
pmid = {22892367},
publisher = {Elsevier Inc.},
title = {{Executive functions and inhibitory control in multilingual children: Evidence from second-language learners, bilinguals, and trilinguals}},
url = {http://dx.doi.org/10.1016/j.jecp.2012.06.013},
volume = {113},
year = {2012}
}
