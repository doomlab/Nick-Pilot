publisher = {Elsevier Inc.},
title = {{Characteristics of brief sticky mittens training that lead to increases in object exploration}},
url = {http://dx.doi.org/10.1016/j.jecp.2017.04.009 http://linkinghub.elsevier.com/retrieve/pii/S0022096516302260},
volume = {164},
