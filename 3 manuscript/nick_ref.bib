month = {jul},
number = {7},
pages = {868--880},
pmid = {27841446},
title = {{Efficiently measuring dimensions of the externalizing spectrum model: Development of the Externalizing Spectrum Inventory-Computerized Adaptive Test (ESI-CAT).}},
