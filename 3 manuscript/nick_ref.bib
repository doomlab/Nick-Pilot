issn = {00220965},
journal = {Journal of Experimental Child Psychology},
keywords = {Biomechanical constraints,Body movement,Eye-tracking,Infant cognition,Pupil size,Robot},
number = {3},
pages = {401--414},
pmid = {22906302},
publisher = {Elsevier Inc.},
title = {{Infant and adult perceptions of possible and impossible body movements: An eye-tracking study}},
url = {http://dx.doi.org/10.1016/j.jecp.2012.07.003},
volume = {113},
year = {2012}
}
@article{Jones2012,
abstract = {Word pairs may be integrative (i.e., combination of two concepts into one meaningful entity; e.g., fruit-cake), thematically related (i.e., connected in time and place; e.g., party-cake), and/or taxonomically related (i.e., shared features and category co-members; e.g., muffin-cake). Using participant ratings and computational measures, we demonstrated distinct patterns across measures of similarity and co-occurrence, and familiarity for each relational construct in two different item sets. In a standard lexical decision task (LDT) with various delays between prime and target presentation (SOAs), target RTs and priming magnitudes were consistent across the three relations for both item sets. However, across the SOAs, there were distinct patterns among the three relations on some of the underlying measures influencing target word recognition (LSA, Google, and BEAGLE). These distinct patterns suggest different mechanisms of lexical priming and further demonstrate that integrative relations are distinct from thematic and taxonomic relations.},
author = {Jones, Lara L. and Golonka, Sabrina},
doi = {10.3389/fnhum.2012.00205},
isbn = {1662-5161},
issn = {1662-5161},
journal = {Frontiers in Human Neuroscience},
