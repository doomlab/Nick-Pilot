pages = {1472--1487},
pmid = {22528370},
title = {{The influence of ethnicity and adverse life experiences during adolescence on young adult socioeconomic attainment: The moderating role of education}},
url = {http://link.springer.com/10.1007/s10964-012-9764-9},
volume = {41},
year = {2012}
