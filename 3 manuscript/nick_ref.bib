pmid = {27611856},
title = {{Examining the psychological effect of rape acknowledgment: The interaction of acknowledgment status and ambivalent sexism}},
url = {http://doi.wiley.com/10.1002/jclp.22379},
volume = {73},
year = {2017}
}
