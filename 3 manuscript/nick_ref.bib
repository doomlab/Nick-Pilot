keywords = {2012,austin f,boston,child molesters,child pornography,department of surgery,internet offenders,lee,ma,massachusetts general hospital,nien-,online first april 16,risk,this article was published},
number = {4},
pages = {644--672},
title = {{Predicting hands-on child sexual offenses among possessors of Internet child pornography.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0027517},
