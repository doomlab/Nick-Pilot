number = {3},
pages = {379--391},
pmid = {22563669},
title = {{Experiences of Christian clients in secular psychotherapy: A mixed-methods investigation.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028283},
volume = {59},
year = {2012}
}
