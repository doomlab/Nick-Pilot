title = {{Preserving integrity in the face of performance threat}},
url = {http://journals.sagepub.com/doi/10.1177/0956797612448483},
volume = {23},
year = {2012}
}
@article{Yen1984,
abstract = {Unidimensional item response theory (IRT) has be come widely used in the analysis and equating of edu cational achievement tests. If an IRT model is true, item responses must be locally independent when the trait is held constant. This paper presents several mea sures of local dependence that are used in conjunction with the three-parameter logistic model in the analysis of unidimensional and two-dimensional simulated data and in the analysis of three mathematics achievement tests at Grades 3 and 6. The measures of local depen dence (called Q2 and Q3) were useful for identifying subsets of items that were influenced by the same fac tors (simulated data) or that had similar content (real data). Item pairs with high Q2 or Q3 values tended to have similar item parameters, but most items with similar item parameters did not have high Q2 or Q3 values. Sets of locally dependent items tended to be difficult and discriminating if the items involved an accumulation of the skills involved in the easier items in the...},
