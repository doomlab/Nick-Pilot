volume = {33},
year = {2011}
}
@article{Hines1986,
abstract = {In three experiments we evaluated semantic priming when the prime was masked below naming threshold and the target was named. Experiment 1 found that when word primes were masked and word targets were named, prior knowledge of the related pairs did not alter semantic priming. How- ever, semantic priming within categories occurred only when the prime stimulus was the first cate- gory exemplar. Experiment 2 found that when masked pictures were used as primes, semantic prim- ing for word targets was highly sensitive to the category exemplar level of the prime but not to the category exemplar level of the target. Word association norms collected in Experiment 3 did not support the hypothesis that the effect of category exemplar level was mediated by the strength of word association. Experiment 4 found significant semantic priming for masked picture primes and within-category word targets, regardless of the level of word association between the prime and target. Experiment 4 also demonstrated semantic priming for high word association targets that were not members of the same semantic category. For all experiments, the subjects with the longest average reaction times also showed the largest semantic priming effect for naming word targets. We hypothe- sized that viewing one of the highest ranking category exemplars activates the memory representa- tion of the category, perhaps because such prototypic exemplars are contained within the category concept itself},
author = {Hines, David and Czerwinski, Mary and Sawyer, Patricia K. and Dwyer, Margaret},
doi = {10.1037/0096-1523.12.3.370},
issn = {0096-1523},
journal = {Journal of Experimental Psychology: Human Perception and Performance},
number = {3},
pages = {370--379},
title = {{Automatic semantic priming: Effect of category exemplar level and word association level.}},
volume = {12},
year = {1986}
}
