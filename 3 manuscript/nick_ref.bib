pmid = {22642393},
title = {{Reliability and validity of the Virtual Reality Lateralized Attention Test in assessing hemispatial neglect in right-hemisphere stroke.}},
url = {https://www.researchgate.net/profile/Laurel{\_}Buxbaum/publication/225071867{\_}Reliability{\_}and{\_}Validity{\_}of{\_}the{\_}Virtual{\_}Reality{\_}Lateralized{\_}Attention{\_}Test{\_}in{\_}Assessing{\_}Hemispatial{\_}Neglect{\_}in{\_}Right-Hemisphere{\_}Stroke/links/02bfe5130dd747454c000000.pdf http://doi.},
