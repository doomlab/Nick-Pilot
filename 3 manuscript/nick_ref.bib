number = {3},
pages = {225--233},
publisher = {Elsevier Ltd},
title = {{How do children cope with global climate change? Coping strategies, engagement, and well-being}},
url = {http://dx.doi.org/10.1016/j.jenvp.2012.02.004},
volume = {32},
year = {2012}
}
