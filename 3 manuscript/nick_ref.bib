keywords = {Symbolic magnitude processing,Number sense,Mathema,dyscalculia,mathematical difficulties,number sense,reliability and,screening,symbolic magnitude processing,validity},
number = {4},
pages = {1361--1373},
publisher = {Behavior Research Methods},
title = {{Symbolic magnitude processing in elementary school children: A group administered paper-and-pencil measure (SYMP Test)}},
