journal = {Psychological Methods},
keywords = {integrative data analysis,measurement,multisample analysis,nonlinear factor},
number = {2},
pages = {101--125},
pmid = {19968394},
title = {{Psychometric approaches for developing commensurate measures across independent studies: Traditional and new models.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0015583},
volume = {14},
year = {2009}
