issn = {00221031},
journal = {Journal of Experimental Social Psychology},
keywords = {Coherence,Spontaneous situational inference,Spontaneous trait inference,Stereotype},
number = {6},
pages = {1247--1256},
publisher = {Elsevier Inc.},
title = {{What I infer depends on who you are: The influence of stereotypes on trait and situational spontaneous inferences}},
