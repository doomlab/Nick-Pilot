@article{Cokley2017,
abstract = {This study investigated whether impostor feelings would both moderate and mediate the relationship between perceived discrimination and mental health in a sample of diverse ethnic minority college students (106 African Americans, 102 Asian Americans, 108 Latino/a Americans) at an urban public university. African American students reported higher perceived discrimination than Asian American and Latino/a American students, while no racial/ethnic group differences were reported for impostor feelings. Analyses revealed that among African American students, high levels of impostor feelings moderated the perceived discrimination and depression relationship and mediated the perceived discrimination and anxiety relationship. Among Asian American students, impostor feelings mediated the relationship between perceived discrimination and both depression and anxiety. Among Latino/a American students low levels of impostor feelings moderated the relationship between perceived discrimination and both depression and anxiety, and partially mediated the relationship between perceived discrimination and anxiety. Multigroup path analyses revealed a significantly stronger impact of impostor feelings on depression among African American students and a stronger impact of perceived discrimination on impostor feelings among African American and Latino/a American students. Clinical implications and future research directions are discussed. Public Significance Statement This study suggests that among ethnic minority students 1 of the reasons perceived discrimination is harmful to mental health is related to feeling like an impostor, which makes the impact of perceived discrimination on mental health much worse. Ethnic minority college students' mental health assessments should include factors related to environmental and academic stressors such as impos-torism and perceived discrimination.},
author = {Cokley, Kevin and Smith, Leann and Bernard, Donte and Hurst, Ashley and Jackson, Stacey and Stone, Steven and Awosogba, Olufunke and Saucer, Chastity and Bailey, Marlon and Roberts, Davia},
doi = {10.1037/cou0000198},
issn = {1939-2168},
journal = {Journal of Counseling Psychology},
keywords = {and low expectations,anxiety,campuses contributes to a,depression,ethnic minority students,feelings of invisibility,impostor phenomenon,many racial,mental health,negative racial climate for,perceived discrimination,perceived discrimination on college,racial segregation},
number = {2},
pages = {141--154},
pmid = {28277731},
title = {{Impostor feelings as a moderator and mediator of the relationship between perceived discrimination and mental health among racial/ethnic minority college students.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/cou0000198},
volume = {64},
year = {2017}
}
@article{Inglis2004,
abstract = {Learning to think logically and present ideas in a logical fashion has always been considered a central part of becoming a mathematician. In this paper we compare the performance of three groups: mathematics undergraduates, mathematics staff and history undergraduates (representative of a ‘general population'). These groups were asked to solve Wason's selection task, a seemingly straightforward logical problem. Given the assumption that logic plays a major role in mathematics, the results were surprising: less than a third of students and less than half of staff gave the correct answer. Moreover, mathematicians seem to make different mistakes from the most common mistake noted in the literature. The implications of these results for our understanding of mathematical thought are discussed with reference to the role of error checking.},
author = {Inglis, Matthew and Simpson, Adrian},
journal = {Proceedings of the 28th Conference of the International Group for the Psychology of Mathematics Education},
pages = {89--96},
title = {{Mathematicians and the selection task}},
volume = {3},
year = {2004}
}
