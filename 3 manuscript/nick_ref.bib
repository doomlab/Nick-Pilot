issn = {2044-5911},
journal = {Journal of Cognitive Psychology},
keywords = {as a field of,association,before psychology was recognised,judgements,learning,memory,researchers used associative memory,science,statistics,structure,tasks to examine memory,techniques},
month = {jun},
