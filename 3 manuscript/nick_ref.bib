@article{Pennebaker1990,
abstract = {On the basis of previous work, freshmen should evidence improved health after writing about their thoughts and feelings associated with entering college. One hundred thirty subjects were assigned to write either about coming to college or about superficial topics for 20 min on 3 days. One fourth of the subjects in each group wrote during the 1st, 5th, 9th, or 14th week of classes. Physician visits for illness in the months after writing were lower for the experimental than for the control subjects. Self-reports of homesickness and anxiety were higher in the experimental group 2-3 months after writing. By year's end, experimental subjects were either superior or similar to control subjects in grade average and in positive moods. No effects emerged as a function of when people wrote, suggesting that the coping process can be accelerated. Implications for comparing insight treatments with catharsis and for distinguishing between objective and self-report indicators of distress are discussed.},
author = {Pennebaker, James W. and Colder, Michelle and Sharp, Lisa K.},
