volume = {41},
year = {2012}
}
@article{Jenkins2017,
abstract = {The ability to exercise patience is important for human functioning. Although it is widely known that patience can be promoted by using willpower to override impatient impulses, patience is also malleable—in particular, susceptible to framing effects—in ways that are difficult to explain on a willpower account alone. So far, the mechanisms underlying framing effects on patience have been elusive. Here we investigated a role for imagination, dissociable from willpower, in these effects. Behaviorally, a classic framing manipulation increased self-reported and independently-coded imagination during intertemporal choice (Experiment 1). Neurally, reframing increased the extent to which patience was associated with activation in brain regions associated with imagination, relative to those associated with willpower, and increased functional connectivity of brain regions associated with imagination, but not willpower, to regions associated with valuation (Experiment 2). Results suggest that reframing can increase the role of imagination in decision-making without increasing willpower exertion.},
author = {Jenkins, Adrianna C. and Hsu, Ming},
doi = {10.1177/0956797617698133},
issn = {0956-7976},
journal = {Psychological Science},
keywords = {13,16,17,20,between immediate temp-,decision making,episodic memory,humans often make trade-offs,impulsivity,neuroimaging,open data,open materials,prospection,received 4,revision accepted 2},
number = {7},
pages = {894--906},
pmid = {28504898},
title = {{Dissociable contributions of imagination and willpower to the malleability of human patience}},
url = {http://journals.sagepub.com/doi/10.1177/0956797617698133},
volume = {28},
year = {2017}
}
