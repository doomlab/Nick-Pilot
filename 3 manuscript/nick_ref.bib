}
@article{Weems2012,
abstract = {In youths, watching TV coverage of a disaster is associated with traumatic-stress symptoms. However, the role of predisaster symptoms in this link has not been addressed. In this study, urban-school youths who had experienced both Hurricanes Katrina and Gustav (N = 141; grades 4-8) were assessed 12 months and 6 months before Gustav and then 1 month after Gustav. The amount of TV viewing was associated with post-Gustav stress symptoms, controlling for pre-Gustav symptoms. However, pre-Gustav stress symptoms interacted with TV viewing in predicting post-Gustav symptoms such that for youths with higher preexisting symptoms, there was a stronger association between TV viewing and level of post-Gustav symptoms. The results advance the literature on the role of media coverage in stress reactions by showing that preexisting symptoms can be an important component of identifying which children are likely to be most negatively affected by TV coverage.},
author = {Weems, Carl F. and Scott, Brandon G. and Banks, Donice M. and Graham, Rebecca A.},
doi = {10.1177/0956797612446952},
isbn = {0956-7976},
issn = {0956-7976},
journal = {Psychological Science},
