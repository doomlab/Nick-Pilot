pages = {258--272},
pmid = {28150974},
title = {{Why are conservatives more punitive than liberals? A moral foundations approach.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28150974{\%}5Cnhttp://doi.apa.org/getdoi.cfm?doi=10.1037/lhb0000232 http://doi.apa.org/getdoi.cfm?doi=10.1037/lhb0000232},
volume = {41},
