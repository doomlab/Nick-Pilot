pages = {631--41},
pmid = {22846035},
publisher = {NIH Public Access},
title = {{Naturalistic assessment of everyday functioning in individuals with mild cognitive impairment: the day-out task.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22846035 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3546511},
volume = {26},
