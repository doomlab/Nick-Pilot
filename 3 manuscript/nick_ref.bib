number = {6},
pages = {1675--1688},
pmid = {22563631},
title = {{Tradeoffs between sequences: Weighing accumulated outcomes against outcome-adjusted delays.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028216},
volume = {38},
year = {2012}
}
