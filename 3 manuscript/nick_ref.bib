month = {mar},
number = {10},
pages = {2262--2269},
title = {{Written emotional disclosure for women with ovarian cancer and their partners: randomised controlled trial}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}PAGE=reference{\&}D=emed11{\&}NEWS=N{\&}AN=2013638129 http://doi.wiley.com/10.1002/pon.3280},
