pages = {1165--1171},
pmid = {22833342},
title = {{The impact of emotion on prospective memory and monitoring: No pain, big gain}},
url = {http://www.springerlink.com/index/10.3758/s13423-012-0301-3},
volume = {19},
year = {2012}
}
