keywords = {anxiety,barr{\'{o}}s-loscertales contributed equally to,behavioral inhibition system,orbitofrontal cortex,paola fuentes and alfonso,personality,precuneus,prefrontal cortex,voxel-based morphometry},
month = {sep},
number = {3},
pages = {491--498},
pmid = {22592859},
title = {{Individual differences in the Behavioral Inhibition System are associated with orbitofrontal cortex and precuneus gray matter volume}},
url = {http://www.springerlink.com/index/10.3758/s13415-012-0099-5 http://www.ncbi.nlm.nih.gov/pubmed/22592859},
