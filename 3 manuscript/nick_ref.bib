isbn = {1016-9040},
issn = {1016-9040},
journal = {European Psychologist},
number = {2},
pages = {85--97},
pmid = {11785152},
title = {{Is Working Memory Still Working? 1Copyright {\textcopyright} 2001 by the American Psychological Association. Reprinted with permission from the original publication: Baddeley A. (2001). “Is Working Memory Still Working?” {\textless}i{\textgreater}American Psychologist, 56,{\textless}/i{\textgreater} 849-864. This r}},
url = {http://econtent.hogrefe.com/doi/abs/10.1027//1016-9040.7.2.85},
