@article{Dervin2014,
abstract = {This article focuses on monitoring political group-fantasy. After tracing a disturbing trend of political and social bullying in 2010, the author began discerning different trends in 2011, which he suggests could be called recoils or counter-balances. He monitored these trends for two years, and explores his observations in the context of United States politics during that timeframe. He summarizes that despite the deadlock on most controversial issues, there was surprising movement. Marriage equality suddenly became a cause whose time has come, the Boy Scouts would no longer discriminate according to sexual orientation, the generals finally listened to women senators on the pandemic of sexual violence in the armed services, and entrenched positions on immigration reform began loosening as politics' strange bed-fellows formed new alliances. Both intriguing and unpredictable, savvy as well as sappy, group processes never rest. (PsycINFO Database Record (c) 2014 APA, all rights reserved)},
author = {Dervin, Dan},
isbn = {0145-3378(Print)},
issn = {0145-3378},
journal = {The Journal of Psychohistory},
keywords = {monitoring political group-fantasy new alliances s},
