journal = {Journal of Consulting and Clinical Psychology},
keywords = {although the college years,are often thought of,as carefree for,at least one mental,blanco et al,borderline personality disorder,college students,dbt,given year,health disorder in a,nearly half of this,population can be diagnosed,suicidality,with,young adults},
