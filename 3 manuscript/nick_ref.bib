journal = {Journal of Consulting and Clinical Psychology},
keywords = {2017,cluster randomized controlled trial,department of psychiatry,depression,felder and elissa epel,group prenatal care,jennifer n,jessica b,lewis,online first march 13,perinatal,preterm birth,san francisco,this article was published,university of california,yale school},
number = {6},
pages = {574--584},
pmid = {28287802},
title = {{Depressive symptoms and gestational length among pregnant adolescents: Cluster randomized control trial of CenteringPregnancy{\textregistered} plus group prenatal care.}},
