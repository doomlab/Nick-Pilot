title = {{Modulation of language switching by cue timing: Implications for models of bilingual language control.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/xlm0000382},
volume = {43},
year = {2017}
}
@article{Marty2012,
abstract = {OBJECTIVES: This study evaluated the structure and validity of the use of the 18-item Interpersonal Needs Questionnaire (INQ-18), a measure of thwarted belongingness (TB) and perceived burdensomeness (PB), among older adults.$\backslash$n$\backslash$nMETHOD: Community-dwelling older adults (N = 284; mean age = 73 years; age range = 64-96 years; 56{\%} women) anonymously completed a questionnaire packet.$\backslash$n$\backslash$nRESULTS: Principal axis factor analysis indicated that two factors should be retained. Items in the first factor reflected the concept of TB, whereas items in the second factor were consistent with the concept of PB. Both factors had medium-to-large positive correlations with hopelessness, depression, suicide ideation, and low meaning in life, providing evidence for convergent validity.$\backslash$n$\backslash$nCONCLUSIONS: Findings from this study support the use of the INQ-18 among community-dwelling older adults.},
