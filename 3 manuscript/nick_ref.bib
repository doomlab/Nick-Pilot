journal = {Journal of Consulting and Clinical Psychology},
keywords = {a number of psychological,cognitive processing therapy,effectiveness,interpersonal assault,interventions have demonstrated,posttraumatic stress disorder,substantial empirical support in,the remediation of posttraumatic,treatment outcome},
