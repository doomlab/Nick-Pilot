@article{Bathgate2012,
abstract = {Achieving expertise in any area requires extensive practice and engagement with the subject one desires to master. As not all practice yields good progress, methods must be found that lead learners to practice effectively. Many experts employ highly tailored practice involving metacognitive processes, but novices rarely engage in frequent and explicit metacognitive strategies during practice. As a result, novice progress may be impeded through repetition of systematic errors and ineffective techniques. Our study provides evidence of the effectiveness of teaching metacognition to novice music students through weekly lessons. Thirty-five adolescent students of six instructors were randomly assigned to metacognitive focus or existing practice teaching conditions. Students receiving metacognitive teaching achieved higher performance ratings when compared with students receiving control instruction, even though practice time did not vary between groups. These results suggest that having students explicitly verbalize and reflect on their learning process produces more efficient practice and greater end performance. (PsycINFO Database Record (c) 2012 APA, all rights reserved) (journal abstract)},
author = {Bathgate, Meghan and Sims-Knight, Judith and Schunn, Christian},
