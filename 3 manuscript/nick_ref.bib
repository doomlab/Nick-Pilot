keywords = {2004,2010,a suspect,camera perspective bias,compared with suspects who,confessions,criminal investigation is highly,even if later retracted,gudjonsson,incriminating,juror decision making,kassin,kassin et al,minorities,never confess,s confession during a,visual attention},
number = {1},
pages = {56--69},
title = {{Conceptualizing defendants as minorities leads mock jurors to make biased evaluations in retracted confession cases.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0029308},
volume = {19},
year = {2013}
}
@article{Lundqvist2017,
abstract = {{\textcopyright} 2017 APA, all rights reserved). Objective: The literature on emotional processing in Parkinson's disease (PD) patients shows mixed results. This may be because of various methodological and/or patient-related differences, such as failing to adjust for cognitive functioning, depression, and/or mood. Method: In the current study, we tested PD patients and healthy controls (HCs) using emotional stimuli across a variety of tasks, including visual search, short-term memory (STM), categorical perception, and emotional stimulus rating. The PD and HC groups were matched on cognitive ability, depression, and mood. We also explored possible relationships between task results and antiparkinsonian treatment effects, as measured by levodopa equivalent dosages (LED), in the PD group. Results: The results show that PD patients use a larger emotional range compared with HCs when reporting their impression of emotional faces on rated emotional valence, arousal, and potency. The results also show that dopaminergic therapy was correlated with stimulus rating results such that PD patients with higher LED scores rated negative faces as less arousing, less negative, and less powerful. Finally, results also show that PD patients display a general slowing effect in the visual search tasks compared with HCs, indicating overall slowed responses. There were no group differences observed in the STM or categorical perception tasks. Conclusions: Our results indicate a relationship between emotional responses, PD, and dopaminergic therapy, in which PD per se is associated with stronger emotional responses, whereas LED levels are negatively correlated with the strength of emotional responses. (PsycINFO Database Record},
author = {Lundqvist, D. and Sv{\"{a}}rd, J. and {Michelg{\aa}rd Palmquist}, {\AA}. and Fischer, H. and Svenningsson, P.},
doi = {10.1037/neu0000371},
issn = {19311559},
journal = {Neuropsychology},
keywords = {Attention,Dopamine,Emotion,Parkinson's disease,Rating},
number = {6},
pages = {605--612},
title = {{Patients with Parkinson's Disease display a dopamine therapy related negative bias and an enlarged range in emotional responses to facial emotional stimuli.}},
volume = {31},
year = {2017}
}
