volume = {28},
year = {2002}
}
@article{Perri2012,
abstract = {OBJECTIVE: The aim of the present study was to investigate the qualitative characteristics of semantic impairment in patients with mild to moderate Alzheimer's disease (AD). In particular, we wanted to verify if subordinate and distinctive concept features are affected earlier than superordinate or shared ones and whether sensory features are more vulnerable than nonsensory ones. Also, we investigated if feature correlation and level of feature occurrence in concept description (dominance) influence the resilience of concept features to degenerative damage.$\backslash$n$\backslash$nMETHODS: We administered a feature-listing task to nine mild and 10 moderate AD patients and 19 matched healthy controls. The concepts utilized were taken from a normative database, which allowed quantifying semantic indexes of the concepts representation, that is, feature type, distinctiveness, correlation, and dominance.$\backslash$n$\backslash$nRESULTS: Data confirmed that although semantic knowledge of concepts is overall impoverished in AD patients, (a) superordinate knowledge is better preserved than other types of knowledge, (b) nonsensory information is more preserved than sensory information, and (c) highly shared features are less affected than distinctive ones. Furthermore, patients' feature production was influenced by dominance and by age of acquisition.$\backslash$n$\backslash$nCONCLUSIONS: These data are in line with theories postulating that among the dimensions along which feature-based semantic representations may vary, those relative to type and distinctiveness are most sensitive to the differential effect of AD. Results also showed a preserved implicit knowledge about how informative a given feature is in these patients and highlighted the importance of early acquisition of concept knowledge for resilience to degenerative damage.},
author = {Perri, Roberta and Zannino, Giandaniele and Caltagirone, Carlo and Carlesimo, Giovanni A.},
doi = {10.1037/a0029302},
isbn = {0894-4105},
issn = {1931-1559},
journal = {Neuropsychology},
keywords = {advances in the study,alzheimer,based on the observation,decades have been largely,feature listing,from a variety of,in the last,neurological dis-,of human semantic memory,of the seman-,s disease,semantic memory,tic memory deficits resulting},
number = {5},
pages = {652--663},
pmid = {22799748},
title = {{Alzheimer's disease and semantic deficits: A feature-listing study.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0029302},
volume = {26},
year = {2012}
}
