number = {6},
pages = {1773--1779},
pmid = {22563634},
title = {{Distributional analysis of the transposed-letter neighborhood effect on naming latency.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028222},
volume = {38},
year = {2012}
}
@article{Plaut1996,
abstract = {A connectionist approach to processing in quasi-regular domains, as exemplified by English word reading, is developed. Networks using appropriately structured orthographic and phonological rep-resentations were trained to read both regular and exception words, and yet were also able to read pronounceable nonwords as well as skilled readers. A mathematical analysis of a simplified system clarifies the close relationship of word frequency and spelling-sound consistency in influencing nam-ing latencies. These insights were verified in subsequent simulations, including an attractor network that accounted for latency data directly in its time to settle on a response. Further analyses of the ability of networks to reproduce data on acquired surface dyslexia support a view of the reading system that incorporates a graded division of labor between semantic and phonological processes, and contrasts in important ways with the standard dual-route account.},
author = {Plaut, David C and McClelland, James L. and Seidenberg, Mark S and Patterson, Karalyn},
doi = {10.1037//0033-295X.103.1.56},
