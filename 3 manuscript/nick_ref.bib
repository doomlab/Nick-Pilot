pages = {906--913},
pmid = {22711579},
title = {{Memory in motion: Movement dynamics reveal memory strength}},
url = {http://www.springerlink.com/index/10.3758/s13423-012-0281-3},
volume = {19},
year = {2012}
}
@book{Mondak2010,
abstract = {Personality and the Foundations of Political Behavior is the first study in more than 30 years to investigate the broad significance of personality traits for mass political behavior. Drawing on the Big Five personality trait framework, Jeffery J. Mondak argues that attention to personality provides a valuable means to integrate biological and environmental influences in rich, nuanced theories and empirical tests of the antecedents of political behavior. Development of such holistic accounts is critical, Mondak contends, if inquiry is to move beyond simple "blank slate" environmental depictions of political engagement. Analyses examining multiple facets of political information, political attitudes, and participation reveal that the Big Five trait dimensions - openness to experience, conscientiousness, extraversion, agreeableness, and emotional stability - produce both direct and indirect effects on a wide range of political phenomena.},
