issn = {17446570},
journal = {Personnel Psychology},
number = {3},
pages = {599--633},
pmid = {25246403},
title = {{Embedding employees early on: The importance of workplace respect.}},
volume = {69},
year = {2016}
}
@article{Gray2016,
abstract = {Are pressure-induced performance errors in experts associated with novice-like skill execution (as predicted by reinvestment/conscious processing theories) or expert execution towards a result that the performer typically intends to avoid (as predicted by ironic processes theory)? The present study directly compared these predictions using a baseball pitching task with two groups of experienced pitchers. One group was shown only their target while the other was shown the target and an ironic (avoid) zone. Both groups demonstrated significantly fewer target hits under pressure. For the target-only group, this was accompanied by significant changes in expertise-related kinematics variables. In the ironic group, the number of pitches thrown in the ironic zone was significantly higher under pressure and there were no significant changes in kinematics. These results suggest that information about an opponent can influence the mechanisms underlying pressure-induced performance errors.},
author = {Gray, Rob and Orn, Anders and Woodman, Tim},
