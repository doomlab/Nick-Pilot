@article{Deboeck2009,
abstract = {The study of intraindividual variability is central to the study of individuals in psychol-ogy. Previous research has related the variance observed in repeated measurements (time series) of individuals to traitlike measures that are logically related. Intraindividual measures, such as intraindividual standard deviation or the coefficient of variation, are likely to be incomplete representations of intraindividual variability. This article shows that the study of intraindividual variability can be made more productive by examining variability of interest at specific time scales, rather than considering the variability of entire time series. Furthermore, examination of variance in observed scores may not be sufficient, because these neglect the time scale dependent relationships between obser-vations. The current article outlines a method of using estimated derivatives to examine intraindividual variability through estimates of the variance and other distributional properties at multiple time scales. In doing so, this article encourages more nuanced discussion about intraindividual variability and highlights that variability and variance are not equivalent. An example with simulated data and an example relating variability in daily measures of negative affect to neuroticism are provided.},
author = {Deboeck, Pascal R. and Montpetit, Mignon A. and Bergeman, C. S. and Boker, Steven M.},
doi = {10.1037/a0016622},
isbn = {1939-1463 (Electronic)},
