journal = {Cognitive Psychology},
keywords = {Animacy,Production,Relationship between production and comprehension,Relative clauses,Similarity-based},
number = {2},
pages = {141--176},
pmid = {22537914},
title = {{Animacy and competition in relative clause production: A cross-linguistic investigation}},
volume = {65},
year = {2012}
}
