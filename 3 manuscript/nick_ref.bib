keywords = {counterfactual,counterfactual thinking,licensing,moral credentials,racial prejudice},
number = {6},
pages = {916--932},
pmid = {23002956},
title = {{Inventing racist roads not taken: The licensing effect of immoral counterfactual behaviors.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0030008},
volume = {103},
year = {2012}
}
