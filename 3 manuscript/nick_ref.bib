publisher = {Elsevier},
title = {{It's the thought that counts over time: The interplay of intent, outcome, stewardship, and legacy motivations in intergenerational reciprocity}},
url = {http://dx.doi.org/10.1016/j.jesp.2017.07.006},
volume = {73},
year = {2017}
}
