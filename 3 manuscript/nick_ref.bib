isbn = {1932-6203},
issn = {19326203},
journal = {PLoS ONE},
number = {12},
pages = {e26752},
pmid = {22163266},
title = {{Temporal patterns of happiness and information in a global social network: Hedonometrics and Twitter}},
volume = {6},
year = {2011}
