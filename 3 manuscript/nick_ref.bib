title = {{Treatment of adult post-traumatic stress disorder using a future-oriented writing therapy approach}},
url = {http://journals.cambridge.org/action/displayAbstract?fromPage=online{\&}aid=7034900 http://www.journals.cambridge.org/abstract{\_}S1754470X09990171},
volume = {2},
