publisher = {John Wiley {\&} Sons, Ltd (10.1111)},
title = {{What do patients with migraine want from acute migraine treatment?}},
url = {http://doi.wiley.com/10.1046/j.1526-4610.2002.0420s1003.x http://www.ncbi.nlm.nih.gov/pubmed/11966858},
volume = {42 Suppl 1},
year = {2002}
