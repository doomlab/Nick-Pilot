eprint = {1611.01212},
isbn = {4146701700699},
issn = {21622973},
journal = {Nature Communications},
number = {1},
pmid = {28506124},
publisher = {Taylor {\&} Francis},
title = {{Tricritical wings and modulated magnetic phases in LaCrGe3under pressure}},
volume = {8},
year = {2017}
}
