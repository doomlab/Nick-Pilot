number = {6},
pages = {636--647},
pmid = {28358554},
title = {{Attenuated hemispheric asymmetry in metaphor processing among adults with ADHD.}},
url = {https://search.proquest.com/docview/1882278280?accountid=14468{\%}0Ahttp://wx7cf7zp2h.search.serialssolutions.com?ctx{\_}ver=Z39.88-2004{\&}ctx{\_}enc=info:ofi/enc:UTF-8{\&}rfr{\_}id=info:sid/ProQ{\%}3Apsycinfo{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:journal{\&}rft.genre=article{\&}rft.jt},
volume = {31},
