number = {6},
pages = {695--703},
pmid = {23106116},
title = {{Constraints on information processing capacity in adults with ADHD.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0030296},
volume = {26},
year = {2012}
