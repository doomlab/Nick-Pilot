pmid = {27685643},
title = {{Effects of a proven error on evaluations of witness testimony.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27685643 http://doi.apa.org/getdoi.cfm?doi=10.1037/lhb0000217},
volume = {41},
