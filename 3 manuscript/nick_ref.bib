journal = {Neuropsychology},
keywords = {and philosoph-,authoritarianism,belief,develop the cognitive capacities,doubt has been an,enduring focus of psychological,for belief and,fundamentalism,how we come to,ical inquiry,lesion,prefrontal cortex,recently,these processes have begun,to be studied},
number = {4},
pages = {414--421},
pmid = {22612576},
title = {{Authoritarianism, religious fundamentalism, and the human prefrontal cortex.}},
