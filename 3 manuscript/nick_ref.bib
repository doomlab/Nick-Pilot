number = {3},
pages = {352--367},
pmid = {22774865},
title = {{Relationships among client and counselor agreement about the working alliance, session evaluations, and change in client symptoms using response surface analysis.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028907},
volume = {59},
year = {2012}
