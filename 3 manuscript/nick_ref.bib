pmid = {28039564},
publisher = {Springer US},
title = {{Identity processes and intrinsic and extrinsic goal pursuits: Directionality of effects in college students}},
url = {http://dx.doi.org/10.1007/s10964-016-0626-8 http://link.springer.com/10.1007/s10964-016-0626-8},
