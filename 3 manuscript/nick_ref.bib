journal = {Cognitive, Affective, {\&} Behavioral Neuroscience},
keywords = {Emotion regulation,Fl,Machiavellianism,Reappraisal,emotion,flexibility,iaps,machiavellianism,reappraisal,regulation,superior temporal gyrus},
number = {3},
pages = {528--541},
pmid = {28105532},
publisher = {Cognitive, Affective, {\&} Behavioral Neuroscience},
title = {{Machiavellian emotion regulation in a cognitive reappraisal task: An fMRI study}},
