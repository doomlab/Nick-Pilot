number = {5},
pages = {842--849},
pmid = {22845782},
title = {{Nonsuicidal self-injury as a time-invariant predictor of adolescent suicide ideation and attempts in a diverse community sample.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0029429},
volume = {80},
