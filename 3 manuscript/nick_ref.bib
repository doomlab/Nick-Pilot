journal = {Journal of Counseling Psychology},
keywords = {1999,academically,and desire to excel,asian americans have been,atkinson,depression,for their ability,group who may be,held to high regard,kim,mediation,minority,model,moderation,perceived burdensomeness,the,thwarted belongingness,viewed as members of,yang},
number = {3},
pages = {280--291},
pmid = {28182491},
title = {{Thwarted belongingness, perceived burdensomeness, and depression among asian americans: A longitudinal study of interpersonal shame as a mediator and perfectionistic family discrepancy as a moderator.}},
