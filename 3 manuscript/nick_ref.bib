doi = {10.1037/ccp0000201},
issn = {1939-2117},
journal = {Journal of Consulting and Clinical Psychology},
keywords = {2017,alcohol,and german centre for,and prevention,brief intervention,cardiovascular research,computer intervention,counseling,grei-,institute of social medicine,medicine greifswald,mental health,online first march 23,sophie baumann,this article was published,university},
number = {6},
pages = {562--573},
title = {{Does impaired mental health interfere with the outcome of brief alcohol intervention at general hospitals?}},
