@article{Phelps2002,
abstract = {In writing the U.S. Constitution the framers anticipated that the use of$\backslash$nAmerican military force should require an extraordinary consensus$\backslash$nbetween Congress and the President. The era of the Vietnam War led many$\backslash$nto believe that Congress had become the junior partner to presidents who$\backslash$nexercised an increasing degree of constitutional independence in use of$\backslash$nforce issues. The War Powers Resolution (WPR) of 1973 attempted to$\backslash$naddress that constitutional imbalance. Our analysis of the$\backslash$nconstitutional debates surrounding the adoption of the WPR reveals that$\backslash$nmembers of Congress were unable to agree upon a coherent$\backslash$n``institutional{\{}''{\}}' vision for Congress; even those who supported the$\backslash$nmeasure were unsure of its meaning. When Congress took up measures$\backslash$nrelated to the commitment of American forces in Kosovo, the same$\backslash$nconstitutional divisions that existed 25 years previously re-emerged,$\backslash$nand Congress was unable to perform as. a full constitutional partner-not$\backslash$nbecause of presidential bullying, but because it lacked its own$\backslash$ninstitutional view of its constitutional responsibilities.},
