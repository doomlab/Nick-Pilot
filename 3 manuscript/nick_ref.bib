title = {{The earth is round (p {\textless} .05)}},
url = {http://www.sjsu.edu/faculty/gerstman/misc/Cohen1994.pdf http://psycnet.apa.org/journals/amp/49/12/997.html{\%}5Cnpapers3://publication/doi/10.1037/0003-066X.49.12.997 http://doi.apa.org/getdoi.cfm?doi=10.1037/0003-066X.50.12.1103 http://doi.apa.org/getdoi.cf},
