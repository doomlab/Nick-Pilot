number = {1},
title = {{Structured writing about a natural disaster buffers the effect of intrusive thoughts on negative affect and physical symptoms.}},
url = {http://search.ebscohost.com/login.aspx?direct=true{\&}db=psyh{\&}AN=2002-14067-003{\&}site=ehost-live{\%}5Cnjmsmyth@psych.syr.edu},
volume = {6},
