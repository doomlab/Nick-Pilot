@article{Daniels2017,
abstract = {Diversity research has long assumed that individuals??? perceptions of diversity are accurate, consistent with normative theories of judgments in economics and decision theory. We challenge this assumption. In six experiments, we show that when there is more diversity along one dimension (e.g., race, clothing color), people also perceive more diversity on other dimensions (e.g., gender, skill) even when this cannot reflect reality. This spillover bias in diversity judgment leads to predictable errors in decision making with economic incentives for accuracy, and it alters support for affirmative action policies in organizations. Spillover bias in diversity judgment may help explain why managerial decisions about groups often appear to be suboptimal and why diversity scholars have found inconsistent associations between objective diversity and team outcomes.},
author = {Daniels, David P. and Neale, Margaret A. and Greer, Lindred L.},
doi = {10.1016/j.obhdp.2016.12.005},
issn = {07495978},
journal = {Organizational Behavior and Human Decision Processes},
keywords = {Decision making,Diversity,Judgment,Spillover bias},
pages = {92--105},
publisher = {Elsevier Inc.},
title = {{Spillover bias in diversity judgment}},
url = {http://dx.doi.org/10.1016/j.obhdp.2016.12.005},
volume = {139},
year = {2017}
}
@article{Morgan2016,
abstract = {En los {\'{u}}ltimos 20 a{\~{n}}os, el estudio de la Psicolog{\'{i}}a Temporal y su relaci{\'{o}}n con una serie de indicadores psicosociales ha aumentado exponencialmente. El presente estudio hace uso de los recientes avances en los m{\'{e}}todos anal{\'{i}}ticos y utiliza la inferencia estad{\'{i}}stica mediante el examen longitudinal de los perfiles latentes a trav{\'{e}}s de an{\'{a}}lisis de transici{\'{o}}n latente (LTA). Las primeras 2 ondas disponibles de datos de un estudio longitudinal en el Reino Unido se utilizaron para evaluar lo siguiente: (a) la estructura de factor de Adolescente Tiempo Inventario Time Attitudes Escala (ATAS) puntuaciones en las olas 1 y 2, (b) la (C) el grado de estabilidad y / o transici{\'{o}}n entre la pertenencia a estos perfiles a los 12 meses, y (d) la relaci{\'{o}}n entre los perfiles y las puntuaciones en 4 resultados distales, Incluyendo la autoeficacia acad{\'{e}}mica, social y emocional, y la b{\'{u}}squeda de sensaciones. Se identificaron cuatro perfiles latentes denominados Positivos, Ambivalentes, Negativos y Futuros Negativos y se examin{\'{o}} su relaci{\'{o}}n con los 4 resultados distales. Los resultados mostraron que los perfiles de actitud temporal eran inestables durante el primer a{\~{n}}o de la escuela secundaria. Adem{\'{a}}s, la inestabilidad se explic{\'{o}} por transiciones a perfiles m{\'{a}}s negativos, y estas transiciones se asociaron con resultados desfavorables.},
author = {Morgan, Grant B. and Wells, Kevin E. and Andretta, James R. and McKay, Michael T.},
doi = {10.1037/pas0000383},
isbn = {1939-134X(Electronic);1040-3590(Print)},
issn = {1939-134X},
journal = {Psychological Assessment},
keywords = {latent transition analysis,mover-stayer model,self-efficacy,time attitude scale},
number = {7},
pages = {890--901},
title = {{Temporal attitudes profile transition among adolescents: A longitudinal examination using mover-stayer latent transition analysis.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/pas0000383},
volume = {29},
