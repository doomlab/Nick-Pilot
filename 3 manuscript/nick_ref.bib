journal = {Journal of Counseling Psychology},
keywords = {based on intentional and,counseling,help seeking,help-,much of the prior,on the assumption that,prototype willingness,reasoned action,reasoned cognitive pro-,research on professional psychological,seeking decisions has operated,the decision,to seek help is},
