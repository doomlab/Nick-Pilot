journal = {Journal of Environmental Psychology},
keywords = {Anxiety,Diagnostic scan,Environmental pleasantness,Motion nature projection,Physiological arousal,Positive distraction},
month = {nov},
pages = {168--176},
title = {{Motion nature projection reduces patient's psycho-physiological anxiety during CT imaging}},
