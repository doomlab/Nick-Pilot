@article{Lou2017,
abstract = {Is inferring readers' literacy skills possible by ana-lyzing their eye movements during text reading? This study used Support Vector Machines (SVM) to analyze eye move-ment data from 61 undergraduate students who read a multi-ple-paragraph, multiple-topic expository text. Forward fixa-tion time, first-pass rereading time, second-pass fixation time, and regression path reading time on different regions of the text were provided as features. The SVM classification algo-rithm assisted in distinguishing high-literacy-skilled readers from low-literacy-skilled readers with 80.3 {\%} accuracy. Results demonstrate the effectiveness of combining eye track-ing and machine learning techniques to detect readers with low literacy skills, and suggest that such approaches can be potentially used in predicting other cognitive abilities.},
