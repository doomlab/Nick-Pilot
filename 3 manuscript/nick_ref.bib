pages = {639--651},
title = {{Field measures of psychopathy and sexual deviance as predictors of recidivism among sexual offenders}},
url = {https://search.proquest.com/docview/1909170932?accountid=11526{\%}0Ahttp://rc4ht3qs8p.search.serialssolution.com?ctx{\_}ver=Z39.88-2004{\&}ctx{\_}enc=info:ofi/enc:UTF-8{\&}rfr{\_}id=info:sid/PsycARTICLES{\&}rft{\_}val{\_}fmt=info:ofi/fmt:kev:mtx:book{\&}rft.genre=unknown{\&}rft.jtitle={\&}r},
