issn = {1573-661X},
journal = {Law and Human Behavior},
keywords = {*Alcohol Intoxication,*Memory,*Recall (Learning),*State Dependent Learning,Witnesses},
number = {2},
pages = {202--215},
title = {{Witness memory and alcohol: The effects of state-dependent recall.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/lhb0000224},
volume = {41},
year = {2017}
}
