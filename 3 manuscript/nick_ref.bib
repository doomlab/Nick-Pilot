keywords = {Lexical centrality,Lexical decision,Semantic network,Semantic relatedness,Word associations},
number = {2},
pages = {480--498},
pmid = {23055165},
primaryClass = {gr-qc},
title = {{Better explanations of lexical and semantic cognition using networks derived from continued rather than single-word associations}},
volume = {45},
year = {2013}
