journal = {Cognitive, Affective and Behavioral Neuroscience},
keywords = {Emotion regulation Event-related potential Late po},
pages = {1--12},
publisher = {Cognitive, Affective, {\&} Behavioral Neuroscience},
title = {{The temporal dynamics of detached versus positive reappraisal: An ERP study}},
year = {2017}
}
