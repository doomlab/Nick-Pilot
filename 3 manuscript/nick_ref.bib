pages = {929--941},
pmid = {25071666},
title = {{Alpha-Band Oscillations enable spatially and temporally resolved tracking of covert spatial attention}},
url = {http://journals.sagepub.com/doi/10.1177/0956797617699167},
volume = {28},
year = {2017}
}
@article{Lakens2017a,
abstract = {Scientists should be able to provide support for the absence of a meaningful effect. Currently, researchers often incorrectly conclude an effect is absent based a nonsignificant result. A widely recommended approach within a frequentist framework is to test for equivalence. In equivalence tests, such as the two one-sided tests (TOST) procedure discussed in this article, an upper and lower equivalence bound is specified based on the smallest effect size of interest. The TOST procedure can be used to statistically reject the presence of effects large enough to be considered worthwhile. This practical primer with accompanying spreadsheet and R package enables psychologists to easily perform equivalence tests (and power analyses) by setting equivalence bounds based on standardized effect sizes and provides recommendations to prespecify equivalence bounds. Extending your statistical tool kit with equivalence tests is an easy way to improve your statistical and theoretical inferences.},
author = {Lakens, Dani{\"{e}}l},
doi = {10.1177/1948550617697177},
issn = {1948-5506},
journal = {Social Psychological and Personality Science},
keywords = {equivalence testing,null hypothesis significance testing,power analysis,research methods},
month = {may},
number = {4},
pages = {355--362},
publisher = {SAGE PublicationsSage CA: Los Angeles, CA},
title = {{Equivalence tests}},
url = {http://journals.sagepub.com/doi/10.1177/1948550617697177},
volume = {8},
year = {2017}
}
@article{Gellaitry2010,
abstract = {Objective: To assess the effects of an expressive writing (EW)$\backslash$nintervention on perceptions of emotional support in women completing$\backslash$ntreatment for early stage breast cancer.$\backslash$nMethods: Women were recruited to the study during their final week of$\backslash$ntreatment. Of 260 eligible patients, 104 (40{\%}) agreed to participate,$\backslash$nand 93 were randomised. Women in the writing group wrote for 20 min on$\backslash$nfour consecutive days. The control group received normal care. Women's$\backslash$nperceptions of emotional support, quality of life (QOL), mood, and$\backslash$nhealthcare utilisation were assessed at baseline, I month, 3 months and$\backslash$n6 months. Interviews were conducted to explore women's experience of$\backslash$nwriting.$\backslash$nResults: Eighty participants completed all follow-ups. There was a$\backslash$nsignificant effect of group on women's perceptions of social support$\backslash$nwith those in the intervention group being more satisfied with the$\backslash$nemotional support they received (p{\textless}0.05). Satisfaction with emotional$\backslash$nsupport was negatively correlated with depression/dejection (p{\textless}0.05) and$\backslash$nanger/hostility (p{\textless}0.05) and positively correlated with social and$\backslash$nfamily well-being (p{\textless}0.001) 6 months post intervention. There were no$\backslash$nsignificant effects of the intervention on mood, QOL or healthcare$\backslash$nutilisation. Most participants found writing valuable and did not report$\backslash$nany long-term negative effects.$\backslash$nConclusion: EW was associated with a higher level of satisfaction with$\backslash$nemotional support compared with controls. Given the existing evidence$\backslash$nsupporting the importance of social support in adjustment to breast$\backslash$ncancer, it seems feasible to suggest that EW may be a cost effective$\backslash$naccessible treatment that could be incorporated into the ongoing care of$\backslash$nwomen. Copyright (C) 2009 John Wiley {\&} Sons, Ltd.},
author = {Gellaitry, Grace and Peters, Keri and Bloomfield, David and Horne, Rob},
doi = {10.1002/pon.1532},
isbn = {1099-1611},
issn = {10579249},
journal = {Psycho-Oncology},
keywords = {11 december 2008,25 june 2008,29 november 2008,accepted,cancer,expressive writing,oncology,psychology,received,revised,social support},
month = {jan},
number = {1},
pages = {77--84},
pmid = {19194996},
title = {{Narrowing the gap: The effects of an expressive writing intervention on perceptions of actual and ideal emotional support in women who have completed treatment for early stage breast cancer}},
url = {http://doi.wiley.com/10.1002/pon.1532},
volume = {19},
year = {2010}
}
@article{Ekehammar2004,
abstract = {Whereas previous research has studied the relation of either (i) personality with prejudice, (ii) personality with social dominance orientation (SDO) and right-wing authoritarianism (RWA), or (iii) SDO and RWA with prejudice, the present research integrates all approaches within the same model. In our study (N183), various causal models of the relationships among the Big Five, SDO, RWA, and Generalized Prejudice are proposed and tested. Generalized Prejudice scores were obtained from a factor analysis of the scores on various prejudice instruments (racism, sexism, prejudice toward homosexuals, and mentally disabled people), which yielded a one-factor solution. The best-fitting causal model, which was our suggested hypothetical model, showed that Big Five personality had no direct effect on Generalized Prejudice but an indirect effect transmitted through RWA and SDO, where RWA seems to capture personality aspects to a greater extent than SDO. Specifically, Generalized Prejudice was affected indirectly by Extraversion, Openness to Experience, and Conscientiousness through RWA, and by Agreeableness through SDO, whereas Neuroticism had no effect at all. The results are discussed against the background of previous research and the personality and social psychology approaches to the study of prejudice.},
author = {Ekehammar, Bo and Akrami, Nazar and Gylje, Magnus and Zakrisson, Ingrid},
doi = {10.1002/per.526},
isbn = {1099-0984},
issn = {0890-2070},
journal = {European Journal of Personality},
month = {sep},
number = {6},
pages = {463--482},
title = {{What matters most to prejudice: Big Five personality, Social Dominance Orientation, or Right-Wing Authoritarianism?}},
url = {http://doi.wiley.com/10.1002/per.526},
volume = {18},
year = {2004}
}
@article{Curtis2016a,
abstract = {A superordinate identity improves intergroup relations and bolsters support for the political system. Yet, why do only some identify superordinately? I argue that personality is an important determinant. I test this using an original survey in the United Kingdom, where European Union integration has increased the salience and feasibility of the ‘‘European'' identity option in addition to a national one. Several Big Five traits matter: openness and extraversion increase identification with Europe while agreeable- ness decreases it. Mediation analysis subsequently shows that personality's effects also travel through the mechanisms of risk aversion, knowledge, and ideology. Results imply that certain predispositions prompt some to be more receptive than others to seeing themselves in superordinate terms and that European identification may be at least partly more primordial than previously thought.},
