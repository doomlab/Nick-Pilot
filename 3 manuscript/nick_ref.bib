pages = {162--169},
pmid = {23088681},
title = {{Differentiating the role of three self-compassion components in buffering cognitive-personality vulnerability to depression among Chinese in Hong Kong.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0030451},
volume = {60},
