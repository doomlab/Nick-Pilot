number = {6},
pages = {613--623},
pmid = {28414497},
title = {{Dual tasking in Parkinson's Disease: Cognitive consequences while walking.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/neu0000331},
volume = {31},
year = {2017}
}
