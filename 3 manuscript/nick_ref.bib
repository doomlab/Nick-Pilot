keywords = {Macbeth effect,Moral cleansing,Moral distress,Video game experience,Violent video games},
number = {6},
pages = {1356--1360},
publisher = {Elsevier Inc.},
title = {{Macbeth and the Joystick: Evidence for moral cleansing after playing a violent video game}},
