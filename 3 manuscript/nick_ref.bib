isbn = {1939-2117},
issn = {1939-2117},
journal = {Journal of Consulting and Clinical Psychology},
keywords = {cognitive-behavioral treatment,constitute an exceptionally important,depression,economically disadvantaged mothers of,ence disproportionately high rates,group to target for,internet intervention,interven-,mothers,of depressive syndromes and,young children experi-},
number = {5},
pages = {739--749},
pmid = {22663903},
title = {{Development and pilot evaluation of an Internet-facilitated cognitive-behavioral intervention for maternal depression.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/a0028820},
volume = {80},
