keywords = {Choice Behavior,Judgment,Learning,Memory,Recall (Learning),choice behavior,judgments of learning,study choice},
number = {1},
pages = {174--179},
pmid = {18605499},
title = {{Evidence that judgments of learning are causally related to study choice.}},
url = {https://www.lib.byu.edu/cgi-bin/remoteauth.pl?url=http://search.ebscohost.com/login.aspx?direct=true{\&}db=psyh{\&}AN=2008-04945-026{\&}site=ehost-live{\&}scope=site{\%}5Cnhttp://jm348@columbia.edu},
volume = {15},
