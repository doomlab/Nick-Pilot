pages = {351--366},
title = {{Religion at work: Evaluating hostile work environment religious discrimination claims.}},
url = {http://navigator-iup.passhe.edu/login?url=http://search.proquest.com/docview/893135238?accountid=11652{\%}5Cnhttp://fn9cr5xf4p.search.serialssolutions.com/?ctx{\_}ver=Z39.88-2004{\&}ctx{\_}enc=info:ofi/enc:UTF-8{\&}rfr{\_}id=info:sid/ProQuest+Dissertations+{\%}26+Theses+Globa},
volume = {23},
