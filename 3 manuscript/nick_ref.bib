issn = {1076-8971},
journal = {Psychology, Public Policy, and Law},
keywords = {adult psychopathy,by,by a hope that,examining,juvenile,offenders,pcl,psychopathic traits in adolescents,psychopathy,starting in the 1990s,surge of interest in,there has been a,this interest is driven,to better treat,understanding the precursors to,we may be able,yv},
