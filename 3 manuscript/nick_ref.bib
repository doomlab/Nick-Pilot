issn = {1939-1315},
journal = {Journal of Personality and Social Psychology},
keywords = {a person feel small,and as if he,and the horizons so,awe,but the desert is,camel drivers one,collective engagement,culture,distant,i,many times,night,said one of the,should remain silent,small self,so huge,that they make,the boy intuitively,ve crossed these sands},
number = {2},
pages = {185--209},
pmid = {28481617},
title = {{Awe, the diminished self, and collective engagement: Universals and cultural variations in the small self.}},
url = {http://doi.apa.org/getdoi.cfm?doi=10.1037/pspa0000087},
volume = {113},
