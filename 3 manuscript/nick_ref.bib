pages = {22--36},
pmid = {27889550},
publisher = {Elsevier Inc.},
title = {{To infinity and beyond: Children generalize the successor function to all possible numbers years after learning to count}},
url = {http://dx.doi.org/10.1016/j.cogpsych.2016.11.002},
volume = {92},
year = {2017}
}
