journal = {Journal of Counseling Psychology},
keywords = {1990,1992,an increased vulnerability to,autonomy,blatt,cognitive-personality vulnerability style,depres-,depression,different terms,exaggerated concerns over interpersonal,harris,nietzel,or au-,relationships and,self-compassion,sion,sociotropy,tonomous achievements lead to,zuroff},
