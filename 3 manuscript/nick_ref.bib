@article{Renfrow2016,
abstract = {Drawing on 251 incident reports, this study explores American football$\backslash$nfans' decisions to rush the field to celebrate a victory despite pleas$\backslash$nfrom university officials and the police to abstain. We explore the$\backslash$nsymbolic interactions through which students defined this situation and$\backslash$nacted within it. Our findings characterize this event as series of$\backslash$nongoing interactions wherein meaning and action are continually$\backslash$n(re)negotiated. Campus rumors normalized the act of rushing by locating$\backslash$nit and the student role within local tradition. Through interactions$\backslash$nwith other students in the stadium and by drawing on knowledge of prior$\backslash$nsports tragedies, fans assessed the risks of participating and selected$\backslash$namong lines of action ranging from ``going to be with others{\{}''{\}} and$\backslash$n``getting out of the way{\{}''{\}} to ``going with the flow.{\{}''{\}} Ultimately,$\backslash$nhowever, public address announcements, the loss of bodily control, and$\backslash$nthe inability to direct other people's actions aligned competing$\backslash$ndefinitions of the situation into one of emergency. We conclude by$\backslash$ndiscussing the theoretical and practical implications of these findings.},
author = {Renfrow, Daniel G. and Wissick, Terrence L. and Guard, Christopher M.},
doi = {10.1123/ssj.2015-0107},
issn = {0741-1235},
journal = {Sociology of Sport Journal},
number = {3},
pages = {250--261},
title = {{(Re)Defining the situation when football fans rush the field.}},
url = {http://journals.humankinetics.com/doi/10.1123/ssj.2015-0107},
volume = {33},
year = {2016}
}
@article{Leveau2012,
abstract = {In this article, we present a set of 12 norms that characterize emotional terms in French, English, German, Spanish, Italian, and Finnish. The high correlation between the norm values in the two emotional dimensions of valence and arousal suggests an interlingual homogeneity of emotional representations and allows a significant metanorm-EMONORM-to be established with 6,383 terms characterized in valence and 4,345 terms characterized in arousal. This metanorm is a resource for creating experimental materials in studies on language and emotions. Furthermore, we perform three tests using EMONORM, with the objectives of (1) identifying basic emotions from their valence and arousal values, (2) determining the orientation of texts referring to positive and negative emotions, and (3) evaluating the intensity of emotions expressed in texts. The results are highly similar to those for human judgments. Finally, we present EMOVAL/SEMOTEX, a Web application for static and dynamic valence and arousal emotional analysis of texts using EMONORM ( http://www.semotex.fr ).},
