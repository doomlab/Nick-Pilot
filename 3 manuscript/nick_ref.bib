journal = {Journal of Consulting and Clinical Psychology},
keywords = {2003,2006,alcohol,college,college students receive campus,common alcohol violations include,hoover,intervention,interventions each year,mandated students,porter,posses-,stepped care,tens of thousands of,violations and mandatory alcohol},
