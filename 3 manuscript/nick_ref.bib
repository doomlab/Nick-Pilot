pages = {311--319},
title = {{Differential associations among alcohol use, depression and perceived life meaning in male and female college students.}},
url = {https://acces.bibl.ulaval.ca/login?url=https://search.ebscohost.com/login.aspx?direct=true{\&}db=rzh{\&}AN=2012155541{\&}lang=fr{\&}site=ehost-live},
