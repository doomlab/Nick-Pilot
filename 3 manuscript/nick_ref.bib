journal = {Journal of Memory and Language},
keywords = {Associative,False memory,Homograph,Priming,Recall,Recognition,Semantic},
number = {1},
pages = {1--28},
title = {{Decoupling semantic and associative information in false memories: Explorations with semantically ambiguous and unambiguous critical lures}},
volume = {52},
year = {2005}
}
